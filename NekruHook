Name.Parent = TopTabFrame
Name.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Name.BackgroundTransparency = 1.000
Name.Position = UDim2.new(0.0388888903, 0, 0, 0)
Name.Size = UDim2.new(0, 173, 0, 21)
Name.Font = Enum.Font.Arial
Name.Text = "NekruHook"
