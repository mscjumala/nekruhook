-- Instances:

local ScreenGui = Instance.new("ScreenGui")
local MainFrame = Instance.new("Frame")
local TopTabFrame = Instance.new("Frame")
local Name = Instance.new("TextLabel")
local Border = Instance.new("Frame")
local MainscriptFrame = Instance.new("Frame")
local TextLabel = Instance.new("TextLabel")
local TextButton = Instance.new("TextButton")
local UICorner = Instance.new("UICorner")
local TextLabel_2 = Instance.new("TextBox")
local TextButton_2 = Instance.new("TextButton")
local UICorner_2 = Instance.new("UICorner")
local TextLabel_3 = Instance.new("TextLabel")
local TextButton_3 = Instance.new("TextButton")
local UICorner_3 = Instance.new("UICorner")
local TextLabel_4 = Instance.new("TextLabel")

local inputser = game:GetService("UserInputService")

--Properties:

ScreenGui.Parent = game.CoreGui
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

MainFrame.Name = "MainFrame"
MainFrame.Parent = ScreenGui
MainFrame.BackgroundColor3 = Color3.fromRGB(61, 61, 61)
MainFrame.BorderColor3 = Color3.fromRGB(83, 83, 83)
MainFrame.BorderSizePixel = 2
MainFrame.Position = UDim2.new(0.109992251, 0, 0.207299277, 0)
MainFrame.Size = UDim2.new(0, 180, 0, 200)
MainFrame.Active = true
MainFrame.Draggable = true

TopTabFrame.Name = "TopTabFrame"
TopTabFrame.Parent = MainFrame
TopTabFrame.BackgroundColor3 = Color3.fromRGB(61, 61, 61)
TopTabFrame.BackgroundTransparency = 0.750
TopTabFrame.BorderSizePixel = 0
TopTabFrame.Size = UDim2.new(0, 180, 0, 20)

