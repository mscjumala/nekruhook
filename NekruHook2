	if JumpPower.BackgroundColor3 == Color3.fromRGB(207, 42, 71) then
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = JumpValue.Text
		JumpPower.BackgroundColor3 = Color3.fromRGB(0, 191, 0)
	else
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
		JumpPower.BackgroundColor3 = Color3.fromRGB(207, 42, 71)
	end
end)

