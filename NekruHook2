local inpser = game:GetService("UserInputService")

inpser.InputBegan:Connect(function(key, g)
	if key.KeyCode == Enum.KeyCode.LeftAlt then
        if Main.Visible = true then
		Main.Visible = false
        elseif Main.Visible = false then
		Main.Visible = true
	end
end)
