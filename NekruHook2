SilentAimFrame.Name = "SilentAimFrame"
SilentAimFrame.Parent = Main
SilentAimFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
SilentAimFrame.BorderColor3 = Color3.fromRGB(54, 54, 54)
SilentAimFrame.BorderSizePixel = 2
SilentAimFrame.Position = UDim2.new(0.0549999997, 0, 0.173999995, 0)
